    "@mui/material": "^7.2.0",
    "@tailwindcss/postcss": "^4.1.5",
    "@tanstack/react-form": "^1.11.1",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-query-devtools": "^5.74.7",
    "case-anything": "^3.1.2",
    "clsx": "^2.1.1",
    "gsap": "^3.13.0",
    "lucide-react": "^0.488.0",
    "material-symbols": "^0.31.3",
    "next": "^16.0.0",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
